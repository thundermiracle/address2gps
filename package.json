{
  "name": "geocoder-free",
  "version": "0.1.0",
  "license": "MIT",
  "description": "get gps by address without using google map api",
  "repository": {
    "type": "git",
    "url": "https://github.com/thundermiracle/address2gps"
  },
  "bugs": {
    "url": "https://github.com/thundermiracle/address2gps/issues"
  },
  "keywords": [
    "geocode",
    "address",
    "gps",
    "google map api",
    "latitude",
    "longitude"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "eslint:src": "eslint src/",
    "eslint:test": "eslint __test__/",
    "eslint": "yarn eslint:src && yarn eslint:test",
    "jest": "jest",
    "test": "yarn eslint && yarn jest && codecov",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "dev": "nodemon --exec babel-node dev",
    "build:rm": "rimraf dist/",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "cross-env NODE_ENV=production babel src --out-dir dist --noEmit --extensions \".ts,.tsx\" --verbose",
    "build": "yarn build:rm && yarn build:types && yarn build:js"
  },
  "author": "thundermiracle",
  "dependencies": {
    "node-fetch": "2.6.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "codecov": "^3.5.0",
    "colors": "^1.3.3",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "typescript": "^3.4.3"
  }
}
